{
  "name": "reducer-composer",
  "description": "utilities for composing reducers",
  "keywords": [
    "reducer",
    "typescript",
    "compose",
    "react",
    "redux"
  ],
  "version": "0.0.6",
  "license": "MIT",
  "author": "Frederik Batuna",
  "private": false,
  "homepage": "https://github.com/freddi301/reducer-composer",
  "repository": {
    "type": "git",
    "url": "https://github.com/freddi301/reducer-composer.git"
  },
  "bugs": {
    "url": "https://github.com/freddi301/reducer-composer/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
